citHeader("To cite evaluationDB in publications use:")

citEntry(entry = "Misc",
  title        = "Teacher Evaluation Data with {evaluationDB}",
  author       = personList(as.person("Lief Esbenshade")),
  year         = "2020",
  url          = "https://github.com/williamlief/evaluationDB",
  note         = "Version 0.1"

  textVersion  =
  paste("Lief Esbenshade (2020).",
        "Teacher Evaluation Data with evaluationDB.",
        "Version 0.1",
        "URL http://www.jstatsoft.org/v40/i03/.")
)